    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "slash-commands": "^1.3.1",
    "ts-jest": "^26.5.2",
    "ts-loader": "^8.0.17",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.2",
    "yaml-cfn": "^0.3.0"
  },
  "dependencies": {
    "@types/express": "^4.17.13",
    "aws-sdk": "^2.850.0",
    "concurrently": "^7.1.0",
    "express": "^4.17.3",
    "nodemon": "^2.0.15",
    "source-map-support": "^0.5.19",
    "tweetnacl": "^1.0.3"
